{
	"name": "vscode-docker-ipython",
	"displayName": "Docker-IPython",
	"repository": "https://github.com/haranjackson/vscode-docker-ipython",
	"description": "Develop interactively with an IPython terminal, running from a locally-defined docker image",
	"version": "0.1.2",
	"publisher": "haranjackson",
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension",
	"contributes": {
		"commands": [
			{
				"command": "docker-ipython.sendFileContentsToIPython",
				"title": "Send File Contents To Docker-IPython"
			},
			{
				"command": "docker-ipython.sendSelectedToIPython",
				"title": "Send Selected Text (or current line) To Docker-IPython"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"uuid": "^3.0.0",
		"mocha": "^2.3.3",
		"eslint": "^3.6.0",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"__metadata": {
		"id": "",
		"publisherId": "",
		"publisherDisplayName": "haranjackson"
	}
}
